// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Result {
  id         String   @id @default(cuid())
  number     String      // 0 to 36 and 00
  createdAt  DateTime @default(now())
}

enum Color {
  RED
  BLACK
  GREEN
}
